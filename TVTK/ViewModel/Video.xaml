<Window x:Class="TVTK.ViewModel.Video"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TVTK.ViewModel"
        xmlns:setting="clr-namespace:TVTK.Controller.Properties"
        xmlns:uc="clr-namespace:LibVLCSharp.WPF;assembly=LibVLCSharp.WPF"
        mc:Ignorable="d"
        Height="450" Width="800" WindowStyle="None" ResizeMode="NoResize"  IsEnabled="False" KeyDown="window_KeyDown">
    <Canvas x:Name="VideoViewCanvas">
        <uc:VideoView x:Name="VideoViewAdv" Panel.ZIndex="2" KeyDown="VideoView_KeyDown" Loaded="VideoViewAdv_Loaded"></uc:VideoView>
        <StackPanel x:Name="NewsPanel" >
            <uc:VideoView x:Name="VideoViewNews" Panel.ZIndex="1" KeyDown="VideoView_KeyDown" Loaded="VideoViewAdv_Loaded" ></uc:VideoView>
        </StackPanel>
    </Canvas>
</Window>
